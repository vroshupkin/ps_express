{
  "name": "ps-express",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set PATH_OPTIONS=\"--experimental-specifier-resolution=node\" && nodemon",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "lint:error": "eslint ./src/** --quiet",
    "start:watch": "rm -rf ./dist && tsc-watch --onSuccess \"node --experimental-specifier-resolution=node ./dist/main.js\"",
    "build": "tsc",
    "dev": "concurrently -k \"tsc --watch \" \"nodemon  --experimental-specifier-resolution=node ./dist/main.js\"",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 --loader ts-node/esm --experimental-specifier-resolution=node ./src/main.ts",
    "doctor": "tsc && set NODE_OPTIONS=\"--experimental-specifier-resolution=node\" && npx clinic doctor --on-port 'autocannon -m POST localhost:8001/users/register' -- node  dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.3"
  }
}
